@model IEnumerable<Verificador.Vista.Models.ImpuestoViewModel>
@if (!(Model == null))
{
    foreach (var item in Model)
    {
        if (item == null)
        {
            continue;
        }
        <div class="col-md-3">
            <div class="alert alert-info alert-dismissible fade in" style="padding:10px 20px 10px 10px" role="alert">
                <button type="button" class="close" data-dismiss="alert" aria-label="Close" onclick="RemoveImpuesto(@(item.Id))">
                    <span aria-hidden="true">×</span>
                </button>
                <strong>@Html.DisplayFor(modelItem => item.Nombre)</strong> @Html.DisplayFor(modelItem => item.Valor)
            </div>
        </div>
    }
}
